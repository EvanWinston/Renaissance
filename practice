#include <stdio.h>
int main()
{
	int n;
	printf("请输入数字1-7任意一个:");
	scanf("%d", &n);
	if (n == 1)
		printf("星期%d对应的英文为:Monday", n);
	if (n == 2)
		printf("星期%d对应的英文为:Tuesday", n);
	if (n == 3)
		printf("星期%d对应的英文为:Wednesday", n);
	if (n == 4)
		printf("星期%d对应的英文为:Thursday", n);
	if (n == 5)
		printf("星期%d对应的英文为:Friday", n);
	if (n == 6)
		printf("星期%d对应的英文为:Saturday", n);
	if (n == 7)
		printf("星期%d对应的英文为:Sunday", n);
	return 0;
}
