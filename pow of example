#include <stdio.h>
#include <math.h>
int main()
{
	long double p1, p2, p3, p4, p5;
	p1 = 1000 * (1 + 0.03) * 5;
	p2 = 1000 * (1 + 2 * 0.021) * (1 + 3 * 0.0275);
	p3 = 1000 * (1 + 3 * 0.0275) * (1 + 2 * 0.021);
	p4 = 1000 * pow(1 + 0.015, 5);
	p5 = 1000 * pow(1 + 0.0035, 4 * 5);
	printf("p1=%lf\np2=%lf\np3=%lf\np4=%lf\np5=%lf\n", p1, p2, p3, p4, p5);
	return 0;
}
